ecera-container{\n  /* margin-bottom: 30px; */\n  /* margin:15px 25px; */\n  /* margin-top: 10px; */\n  width: 100%;\n  /* font-size: 1.4rem; */\n}\n.cabecera-container li{\n  /* display: inline-block; */\n  list-style: none;\n}\n.cabecera-container a{\n  /* display: inline-block; */\n  text-decoration: none;\n}\n.cabecera-itemsRedes li{\n  display: inline-block;\n  /* list-style: none; */\n}\n.cabecera-itemsRedes img{\n  width: 24px;\n  transition: transform .2s;\n}\n.cabecera-itemsRedes img:hover{\n  transform: scale(1.1);\n}\n.cabecera-logo{\n  font-size: 2.6rem;\n  padding: 8px;\n}\n.cabecera-logo img{\n  width: 22px;\n}\n.cabecera-itemsNavegacion li{\n  /* list-style: none; */\n  text-transform: uppercase;\n  /* font-size: 1.3rem; */\n  font-weight: bold;\n  line-height: 20px;\n  /* border-bottom: dotted black 1px;\n  border-top: dotted black 1px; */\n}\n\n.bannerEmpresa{\n  background-color: #017FB1;\n  color: white;\n}\n.bannerEmpresa-bloque{\n  padding-top: 15px;\n  padding-bottom: 10px;\n  border-top: dotted white 1px;\n}\n\n.contenidoCentral{\n  width: 75%;\n  margin:auto;\n  padding-bottom: 15px;\n}\n\n.book .book-image {\n  width: 100%;\n  height: 225px;\n  position: relative;\n  background-repeat: no-repeat;\n  background-position: 50% 50%;\n}\n.book .book-label.label-blue {\n  /*background: #966fee;*/\n  background: #046380;\n}\n.book .book-label.label-bluerain{\n  background: #066;\n}\n.book .book-label.label-yellow {\n  background: #edbe48;\n}\n.book .book-label {\n  height: 20px;\n  overflow: hidden;\n  position: absolute;\n  bottom: 0;\n  text-align: center;\n  color: #fff;\n  -webkit-transition: all .2s ease-in-out;\n  transition: all .2s ease-in-out;\n}\n.book .book-label {\n  height: 20px;\n  overflow: hidden;\n  position: absolute;\n  bottom: 0;\n  text-align: center;\n  color: #fff;\n  -webkit-transition: all .2s ease-in-out;\n  transition: all .2s ease-in-out;\n  border-top-left-radius: 10px;\n  border-top-right-radius: 10px;\n  border-bottom-left-radius: 10px;\n  border-bottom-right-radius: 10px;\n}\n.book:hover .book-label {\n  height: 100%;\n  border-radius: 10px 10px 0 0;\n}\n.book .book-label p {\n  padding: 0 15px;\n}\n.book .book-by span {\n  color: #5586e2;\n}\n.book .book-title {\n  padding: 10px 20px;\n  text-transform: uppercase;\n  text-align: center;\n  font-size: 13px;\n  line-height: 16px;\n  margin-top: 10px;\n  margin-bottom: 0;\n}\n\n.book .book-by {\n  font-size: 12px;\n  color: #7c8e9c;\n  text-align: center;\n}\n.book .book-number {\n  text-align: center;\n  font-weight: 300;\n  font-size: 16px;\n}"},"lineCount":null}},"error":null,"hash":"432513a47f6903d1527a3b78d7fa8df8","cacheData":{"env":{}}}